{
  "name": "@astack-tech/components",
  "version": "0.1.1-beta.4",
  "description": "Components for the Astack AI Framework.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./text": {
      "types": {
        "import": "./dist/text/index.d.ts",
        "require": "./dist/text/index.d.cts"
      },
      "import": "./dist/text/index.js",
      "require": "./dist/text/index.cjs"
    },
    "./agents": {
      "types": {
        "import": "./dist/agents/index.d.ts",
        "require": "./dist/agents/index.d.cts"
      },
      "import": "./dist/agents/index.js",
      "require": "./dist/agents/index.cjs"
    },
    "./tool-invoker": {
      "types": {
        "import": "./dist/tool-invoker/index.d.ts",
        "require": "./dist/tool-invoker/index.d.cts"
      },
      "import": "./dist/tool-invoker/index.js",
      "require": "./dist/tool-invoker/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rimraf dist"
  },
  "keywords": [
    "ai-framework",
    "ai-agent",
    "ai"
  ],
  "author": "xiaoa",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.2",
    "eslint": "^8.56.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@astack-tech/core": "workspace:*",
    "@astack-tech/tools": "workspace:*"
  }
}
